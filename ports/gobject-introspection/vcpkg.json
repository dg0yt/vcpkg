{
  "name": "gobject-introspection",
  "version": "1.72.0",
  "port-version": 5,
  "description": [
    "A middleware layer between C libraries (using GObject) and language bindings.",
    "For triplets with static library linkage, only the tools can be built."
  ],
  "homepage": "https://gi.readthedocs.io/en/latest/",
  "license": null,
  "supports": "!staticcrt",
  "dependencies": [
    "glib",
    {
      "name": "glib",
      "host": true
    },
    {
      "name": "gobject-introspection",
      "host": true
    },
    {
      "name": "gobject-introspection",
      "features": [
        "libs"
      ],
      "platform": "!static"
    },
    "libffi",
    {
      "name": "python3",
      "host": true
    },
    {
      "name": "vcpkg-tool-meson",
      "host": true
    }
  ],
  "features": {
    "libs": {
      "description": "Build libs and introspection data.",
      "supports": "!static"
    }
  }
}
